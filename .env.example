# API Keys Configuration
# Copy this file to .env and fill in your actual keys

# Groq API Key (Free LLM - 14,400 requests/day)
GROQ_API_KEY=your_groq_api_key_here

# LangSmith API Key (Optional - for monitoring/debugging)
# Only enable if you want to use LangSmith for development
LANGCHAIN_TRACING_V2=false
LANGCHAIN_API_KEY=your_langsmith_api_key_here
LANGCHAIN_PROJECT=rag-chatbot-dev

# Note: LangSmith is optional and paid. 
# Set LANGCHAIN_TRACING_V2=true only if you want to enable monitoring
